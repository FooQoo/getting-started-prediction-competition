[[source]]
name = "pypi"
url = "https://pypi.org/simple"
verify_ssl = true

[dev-packages]

[packages]
kaggle = "*"
tqdm = "*"
jupyterlab = "*"
pandas = "*"
nltk = "*"
scikit-learn = "*"
joblib = "*"

[scripts]
jupyter="jupyter lab --no-browser --port=8888 --ip=0.0.0.0 --allow-root --NotebookApp.token=''"
kaggle-list="kaggle competitions list"
kaggle-download="kaggle competitions download -c 'nlp-getting-started' -w"
kaggle-submit="kaggle competitions submit -c nlp-getting-started -f i ./output/submit.csv"

[requires]
python_version = "3.8"
